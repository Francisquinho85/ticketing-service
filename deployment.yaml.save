---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: app
  namespace: app-ticketing
spec:
  selector:
    matchLabels:
      app: app
  template:
    metadata:
      labels:
        app: app
    spec:
      containers:
      - name: app-ticketing
        image: registry.deti:5000/a
        resources:
          requests:
            memory: "32Mi"
            cpu: "10m"
          limits:
            memory: "128Mi"
            cpu: "500m"
        ports:
        - containerPort: 8080
